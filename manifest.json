{
    "name": "Beak-A-Boo",
    "version": "1.0.0",
    "description": "A Chrome Extension to Help You Take Study Breaks!",
    "permissions": ["alarms", "commands", "storage", "declarativeContent", "notifications"],  
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        {
          "resources": [ "shovel.png" ],
          "matches": [ "<all_urls>" ]
        }
    ]
}